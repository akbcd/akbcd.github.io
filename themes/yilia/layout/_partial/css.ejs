<%- css([theme.css.main,theme.css.mobile,theme.css.photoswipe,theme.css.photoswipe_default_skin]) -%>
<%_ if (theme.music && theme.aplayer.enable || theme.music && theme.mobile_aplayer.enable) { _%>
<link rel="stylesheet" type="text/css" href="<%- theme.css.aplayer %>">
<style type="text/css">
  #aplayer ::-webkit-scrollbar-button,#aplayer ::-webkit-scrollbar-track{
    display: none;
  }
</style>
<%_ } if (theme.valine && theme.valine.enable){ _%>
<link rel="stylesheet" type="text/css" href="<%- theme.valine_css %>">
<%_ } if (theme.gitalk && theme.gitalk.enable){ _%>
<link rel="stylesheet" type="text/css" href="<%- theme.gitalk_css %>">
<%_ } if (theme.gitment_owner && theme.gitment_repo &&theme.gitment_oauth && theme.gitment_oauth.client_id && theme.gitment_oauth.client_secret){ _%>
<link rel="stylesheet" type="text/css" href="//imsun.github.io/gitment/style/default.css">
<%_ } if (theme.waline && theme.waline.enable){ _%>
<link rel="stylesheet" type="text/css" href="<%- theme.waline_css %>"><%_ } _%>