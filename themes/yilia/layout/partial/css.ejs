<%- css(["css/main.css","css/mobile.css","css/iconfont.css","css/com.css"]) -%>
<%- css([theme.css.photoswipe,theme.css.photoswipe_default_skin]) -%>
<% if (theme.music && theme.aplayer.enable || theme.music && theme.mobile_aplayer.enable) { %>
  <link rel="stylesheet" type="text/css" href="<%- theme.css.aplayer %>">
  <style type="text/css">
    #aplayer ::-webkit-scrollbar-button,#aplayer ::-webkit-scrollbar-track{
      display: none;
    }
  </style>
<% } if (theme.valine && theme.valine.enable){ %>
  <link rel="stylesheet" type="text/css" href="css/lib/valine/valine.css">
<% } if (theme.gitalk && theme.gitalk.enable){ %>
  <link rel="stylesheet" type="text/css" href="<%- theme.css.gitalk %>">
<% } if (theme.gitment_owner && theme.gitment_repo &&theme.gitment_oauth && theme.gitment_oauth.client_id && theme.gitment_oauth.client_secret){ %>
  <link rel="stylesheet" type="text/css" href="<%- theme.css.gitment %>">
<% } if (theme.waline && theme.waline.enable){ %>
  <link rel="stylesheet" type="text/css" href="<%- theme.css.waline %>">
<% } %>